{
  "name": "towerproperty",
  "version": "1.0.0",
  "description": "Land property consultancy company",
  "main": "server.js",
  "scripts": {
    "db-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate &&  sequelize db:seed:undo:all && sequelize db:seed:all",
    "server": "babel-watch ./server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\"  \"npm run client \"",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "prod": "npm run db-migrate && npm run server"
  },
  "keywords": [
    "nodejs",
    "consultancy",
    "express"
  ],
  "author": "Moise1",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "babel-watch": "^7.0.0",
    "bcrypt": "^5.0.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "localStorage": "^1.0.4",
    "lodash": "^4.17.20",
    "node-localstorage": "^2.1.6",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "store2": "^2.12.0"
  }
}
